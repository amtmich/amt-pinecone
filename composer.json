{
  "name": "amt/amt-pinecone",
  "description": "AMT_Pinecone Extension",
  "type": "typo3-cms-extension",
  "keywords": [
    "TYPO3 CMS",
    "Pinecone",
    "AMT",
    "amt",
    "pineconeapi"
  ],
  "require": {
    "typo3/cms-core": "^12.4",
    "symfony/http-client": "^7.1"
  },
  "autoload": {
    "psr-4": {
      "Amt\\AmtPinecone\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "amt_pinecone"
    },
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true,
      "phpstan/extension-installer": true
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "3.64.*",
    "phpstan/phpstan": "1.12.*",
    "phpstan/extension-installer": "1.4.*",
    "saschaegerer/phpstan-typo3": "1.10.*",
    "captainhook/captainhook": "5.23.*"
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix",
    "phpstan": "phpstan analyse",
    "phpstan-container": "/var/www/html/vendor/bin/phpstan analyse -c phpstan.neon",
    "cache-clear": [
      "ddev exec rm -rf var/cache",
      "ddev exec vendor/bin/typo3 cache:flush"
    ]
  }
}
